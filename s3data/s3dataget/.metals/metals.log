      "/hdfs",
      "/c#",
      "/sdk",
      "/aws",
      "/computing",
      "/data",
      "/apache",
      "/kafka",
      "/mongo"
    )

    val jobSiteIndex = df
      .filter(
        ($"content_languages" === "eng") and ($"content_charset" === "UTF-8") and (($"url_path"
          .rlike(jobsRegex)) and ($"url_path".rlike(techJobs.mkString("|"))))
      )
      .select(
        $"url",
        $"warc_filename",
        $"warc_record_offset",
        $"warc_record_length"
      )

