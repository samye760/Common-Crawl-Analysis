        )
      )
      .load()

    val exampleFormat = df
      .filter(
        ($"url_path" contains "jobs") and ($"content_languages" === "eng")
      )
      .select($"url_host_name", $"url_path" as "sample_path")
      .groupBy("url_host_name", "sample_path")
